    break

fruits=[ "Banan",'alma','almurut','juzum']
jechuu=input('Vash fruct:  ')
for jechuu in fruits:
    if jechuu== "alma":
        print('onun fructysy ALMA')
        break
    elif jechuu== "juzum":
        print('onuku JUZUM eken ')
        break


len= int(input("Entry the lenght of fruits: "))

frukty=[]
for i in range(len):
    name=input(f"name of fruits {i+1}: ")
    frukty.append(name)
    print("Fruits list:", frukty)

 



    
    
